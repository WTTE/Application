.contents {
  width: 93%;
  margin: auto;
  margin-top: 30rpx;
  border: 2rpx solid #EEEEEE;
}
.contents .content1 {
  height: 440rpx;
}
.contents .content1 swiper {
  height: 196px;
}
.contents .content1 swiper image {
  width: 348px;
  height: 210px;
}
.contents .word {
  margin-top: -20rpx;
  margin-left: 20rpx;
}
.contents .word .laber {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.contents .word .laber .reserve {
  color: #ff6c36;
  background-color: #fff6f3;
}
.contents .word .laber text {
  overflow: hidden;
  white-space: nowrap;
  color: #777777;
  padding: 5rpx;
  display: block;
  font-size: 25rpx;
  background-color: #f8f8f7;
  margin-right: 8rpx;
}
.contents .word .title {
  color: #00004f;
  margin-top: 20rpx;
  display: block;
  font-size: 42rpx;
  font-weight: 300;
  margin-bottom: 20rpx;
}
.contents .word1 {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.contents .word1 text {
  color: #b7b7b7;
  display: block;
  margin-bottom: 40rpx;
}

